      <link caption="–°–ø–∞—Å—ã–ª–∫–∞ –Ω–∞ —Ñ–∞–π–ª —Å—Ç—ã–ª—è—û"/>
      <embed caption="–£–±—É–¥–∞–≤–∞—Ü—å —Å—Ç—ã–ª—ñ —û —ç–∫—Å–ø–∞—Ä—Ç–∞–≤–∞–Ω—ã —Ñ–∞–π–ª"/>
    </handling>
    <options caption="–ü–∞—Ä–∞–º–µ—Ç—Ä—ã —Ç–∞–±–ª—ñ—Ü—ã —Å—Ç—ã–ª—è—û CSS">
      <default caption="–í—ã–∫–∞—Ä—ã—Å—Ç–æ—û–≤–∞—Ü—å —É–∫–∞–∑–∞–Ω—É—é —Ç–∞–±–ª—ñ—Ü—É —Å—Ç—ã–ª—è—û"/>
      <print caption="–í—ã–∫–∞—Ä—ã—Å—Ç–æ—û–≤–∞—Ü—å —Å—Ç—ã–ª—å, –∑–∞—Å–Ω–∞–≤–∞–Ω—ã –Ω–∞ –Ω–∞–ª–∞–¥–∞—Ö –¥—Ä—É–∫—É"/>
    </options>
    <export caption="–ú–Ω–æ–∂–Ω—ã —ç–∫—Å–ø–∞—Ä—Ç">
