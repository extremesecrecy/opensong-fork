      <save>–ü–∞–º—ã–ª–∫–∞ –ø—Ä—ã –∑–∞—Ö–∞–≤–∞–Ω–Ω—ñ –∑–º–µ–Ω–µ–Ω–∞–≥–∞ —Ñ–∞–π–ª–∞ '%1'</save>
    </errors>
    <load_successful>–ë—ñ–±–ª—ñ—è '%1' –ø–∞—Å–ø—è—Ö–æ–≤–∞ –∑–∞–≥—Ä—É–∂–∞–Ω–∞</load_successful>
    <messages>
      <bnames xml:space="preserve">–î–∞ –≥—ç—Ç–∞–≥–∞ –ø–µ—Ä–∞–∫–ª–∞–¥—É –Ω–µ–∞–±—Ö–æ–¥–Ω–∞ –¥–∞–¥–∞—Ü—å –Ω–∞–∑–≤—ã –∫–Ω—ñ–≥.
–£–∫–∞–∂—ã—Ü–µ –∞–¥–ø–∞–≤–µ–¥–Ω—É—é –º–æ–≤—É –¥–ª—è –Ω–∞–∑–≤–∞—û.</bnames>
      <save>–ó–∞—Ö–∞–≤–∞—Ü—å –∑–º–µ–Ω–µ–Ω—ã —Ñ–∞–π–ª?</save>
    </messages>
  </bible>

  <song>
    <errors>
      <unknown_element>–ù–µ–≤—è–¥–æ–º—ã —ç–ª–µ–º–µ–Ω—Ç —Å–ø–µ–≤—É: '%1'</unknown_element>
    </errors>
  </song>

