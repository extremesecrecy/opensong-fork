  <slide_editor>
    <header caption="–†—ç–¥–∞–∫—Ç–∞—Ä —Å–ª–∞–π–¥–∞—û"/>
    <title caption="–ó–∞–≥–∞–ª–æ–≤–∞–∫">–ó–∞–¥–∞–µ –∑–∞–≥–∞–ª–æ–≤–∞–∫ —Å–ª–∞–π–¥–∞, —è–∫—ñ –±—É–¥–∑–µ –∞–¥–ª—é—Å—Ç—Ä–æ—û–≤–∞—Ü—Ü–∞ –Ω–∞ —Å–∞–º—ñ–º —Å–ª–∞–π–¥–∑–µ</title>
    <subtitle caption="–ü–∞–¥–∑–∞–≥–∞–ª–æ–≤–∞–∫">–ó–∞–¥–∞–µ –ø–∞–¥–∑–∞–≥–∞–ª–æ–≤–∞–∫ —Å–ª–∞–π–¥–∞, —è–∫—ñ –±—É–¥–∑–µ –∞–¥–ª—é—Å—Ç—Ä–æ—û–≤–∞—Ü—Ü–∞ –Ω–∞ —Å–∞–º—ñ–º —Å–ª–∞–π–¥–∑–µ</subtitle>
    <name caption="–ù–∞–∑–≤–∞">–ó–∞–¥–∞–µ –∑–∞–≥–∞–ª–æ–≤–∞–∫ —Å–ª–∞–π–¥–∞(-–∞—û), —è–∫—ñ –±—É–¥–∑–µ –∞–¥–ª—é—Å—Ç—Ä–æ—û–≤–∞—Ü—Ü–∞ —û –∞–∫–Ω–µ –ø–∞–º–æ—á–Ω—ñ–∫–∞ –ø–∞–∫–∞–∑—É. –Å–Ω –Ω–µ –±—É–¥–∑–µ –∞–¥–ª—é—Å—Ç—Ä–æ—û–≤–∞—Ü—Ü–∞ –Ω–∞ —Å–∞–º—ñ–º(-—ñ—Ö) —Å–ª–∞–π–¥–∑–µ(-–∞—Ö).</name>
    <slides caption="–°–ª–∞–π–¥—ã">–ó–º—è—à—á–∞–µ –∑–º–µ—Å—Ç —Å–ª–∞–π–¥–∞(-–∞—û) —É –≥—ç—Ç—ã–º –∫–∞—Ä—ã—Å—Ç–∞–ª—å–Ω—ñ—Ü–∫—ñ–º —Å–ª–∞–π–¥–∑–µ. –ü–∞–¥–∫–∞–∑–∫–∞: –∫–∞–± —Ö—É—Ç–∫–∞ –¥–∞–¥–∞—Ü—å –≤—ñ–¥–∞—Ä—ã—Å—ã, –ø–µ—Ä–∞—Ü—è–≥–Ω—ñ—Ü–µ —ñ—Ö —É –≥—ç—Ç–∞ –ø–æ–ª–µ.</slides>
    <presentation caption="–ü–∞—Ä–∞–¥–∞–∫ –ø–∞–∫–∞–∑—É">–ü–µ—Ä–∞–≤—ã–∑–Ω–∞—á–∞–µ –ø–∞—Ä–∞–¥–∞–∫ –ø–∞–∫–∞–∑—É, –≤—ã–∑–Ω–∞—á–∞–Ω—ã —û —Ñ–∞–π–ª–µ —Å–ø–µ–≤—É</presentation>
    <origorder caption="–ê—Ä—ã–≥—ñ–Ω–∞–ª—å–Ω—ã –ø–∞—Ä–∞–¥–∞–∫ –ø–∞–∫–∞–∑—É">–ü–∞—Ä–∞–¥–∞–∫ –ø–∞–∫–∞–∑—É, –≤—ã–∑–Ω–∞—á–∞–Ω—ã —û —Ñ–∞–π–ª–µ —Å–ø–µ–≤—É</origorder>
