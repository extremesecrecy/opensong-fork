      <paste caption="link(shared/insert/@caption)">–£—Å—Ç–∞–≤—ñ—Ü—å —Å–∫–∞–ø—ñ—Ä–∞–≤–∞–Ω—ã —ç–ª–µ–º–µ–Ω—Ç –ø–∞—Å–ª—è –≤—ã–±—Ä–∞–Ω–∞–≥–∞</paste>
      <store caption="–ó–∞—Ö–∞–≤–∞—Ü—å">–ó–∞—Ö–æ—û–≤–∞–µ –≤—ã–±—Ä–∞–Ω—ã —ç–ª–µ–º–µ–Ω—Ç —É –Ω–∞–±–æ—Ä—ã</store>
      <presentitem caption="link(sets_mode/current_set/present/@caption)">–ü–∞–∫–∞–∑–≤–∞–µ –±—è–≥—É—á—ã –Ω–∞–±–æ—Ä –Ω–∞ —ç–∫—Ä–∞–Ω–µ, –ø–∞—á—ã–Ω–∞—é—á—ã –∑ –≤—ã–±—Ä–∞–Ω–∞–≥–∞ —ç–ª–µ–º–µ–Ω—Ç–∞. –í—ã –º–æ–∂–∞—Ü–µ –≤—ã–±—Ä–∞—Ü—å –ø–∞–º—ñ–∂ –ø–∞–∫–∞–∑–∞–º –Ω–∞ –±—è–≥—É—á—ã–º —ç–∫—Ä–∞–Ω–µ, –Ω–∞ –¥–∞–¥–∞—Ç–∫–æ–≤—ã–º —ç–∫—Ä–∞–Ω–µ –∞–±–æ –ø–µ—Ä–∞–¥–ø—Ä–∞–≥–ª—è–¥–∞–º –¥–≤–∞–π–Ω–æ–≥–∞ —ç–∫—Ä–∞–Ω–∞ –Ω–∞ –∞–¥–Ω—ã–º —ç–∫—Ä–∞–Ω–µ.
        <single_screen caption="link(sets_mode/current_set/present/single_screen/@caption)"/>
        <dual_screen caption="link(sets_mode/current_set/present/dual_screen/@caption)"/>
        <preview_dual_screen caption="link(sets_mode/current_set/present/preview_dual_screen/@caption)"/>
        <no_slides>link(sets_mode/current_set/present/no_slides)</no_slides>
      </presentitem>
    </current_item>
    <items caption="–≠–ª–µ–º–µ–Ω—Ç—ã –Ω–∞–±–æ—Ä—É">–ü–µ—Ä–∞–ª—ñ—á–≤–∞–µ —ç–ª–µ–º–µ–Ω—Ç—ã —û –±—è–≥—É—á—ã–º –Ω–∞–±–æ—Ä—ã. –ù–∞—Ü—ñ—Å–Ω—ñ—Ü–µ –Ω–∞ –∫–∞—Ä—ã—Å—Ç–∞–ª—å–Ω—ñ—Ü–∫—ñ —Å–ª–∞–π–¥, –∫–∞–± –∑–∞–≥—Ä—É–∑—ñ—Ü—å —è–≥–æ —û —Ä—ç–¥–∞–∫—Ç–∞—Ä –∫–∞—Ä—ã—Å—Ç–∞–ª—å–Ω—ñ—Ü–∫—ñ—Ö —Å–ª–∞–π–¥–∞—û, –∞–±–æ –Ω–∞—Ü—ñ—Å–Ω—ñ—Ü–µ –¥–≤–æ–π—á—ã –Ω–∞ —Å–ø–µ—û, –∫–∞–± –∑–∞–≥—Ä—É–∑—ñ—Ü—å —è–≥–æ —û —Ä—ç–¥–∞–∫—Ç–∞—Ä —Å–ø–µ–≤–∞—û.
      <song caption="(—Å–ø–µ—û)"/>
      <custom caption="(–∫–∞—Ä—ã—Å—Ç–∞–ª—å–Ω—ñ—Ü–∫—ñ)"/>
      <scripture caption="(–ë—ñ–±–ª—ñ—è)"/>
      <image caption="(–≤—ñ–¥–∞—Ä—ã—Å)"/>
      <external caption="(–∑–Ω–µ—à–Ω—ñ)"/>
      <style caption="(—Å—Ç—ã–ª—å)">-- –ó–º–µ–Ω–∞ —Å—Ç—ã–ª—é --</style>
      <revert caption="(—Å—Ç—ã–ª—å)">-- –í—è—Ä—Ç–∞–Ω–Ω–µ —Å—Ç—ã–ª—é --</revert>
      <defaultstyle caption="(—Å—Ç—ã–ª—å)">-- –≠–∫—Å–ø–∞—Ä—Ç–∞–≤–∞–Ω—ã —Å—Ç—ã–ª—å --</defaultstyle>
    </items>
    <untitled caption="–ë–µ–∑ –Ω–∞–∑–≤—ã"/>
    <errors>
      <revert caption="–ü–∞–º—ã–ª–∫–∞ –∑–∞–≥—Ä—É–∑–∫—ñ –Ω–∞–±–æ—Ä—É &quot;%1&quot; –∑ —Ñ–∞–π–ª–∞"/>
      <insert caption="–ù–µ —û–¥–∞–ª–æ—Å—è —û—Å—Ç–∞–≤—ñ—Ü—å —Å–∫–∞–ø—ñ—Ä–∞–≤–∞–Ω—ã—è —Å–ª–∞–π–¥—ã:&#10;%1"/>
    </errors>
