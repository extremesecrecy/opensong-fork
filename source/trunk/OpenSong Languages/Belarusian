  <help_topics title="–¢—ç–º—ã –¥–∞–≤–µ–¥–∫—ñ">
    <tabs head1="–ö–ª–∞–≤—ñ—à—ã –∫—ñ—Ä–∞–≤–∞–Ω–Ω—è –ø–∞–∫–∞–∑–∞–º" head2="–§–∞—Ä–º–∞—Ç –∞–∫–æ—Ä–¥–∞—û/—Ç—ç–∫—Å—Ç—É" head3="–ü–∞—Ä–∞–¥–∞–∫ –ø–∞–∫–∞–∑—É" head4="–ó–∞–∫—Ä—ã—Ü—å"/>
    <print caption="–î—Ä—É–∫"/>
    <close caption="link(shared/close/@caption)"/>
    <keys><![CDATA[
<h1>–ö–ª–∞–≤—ñ—à—ã –∫—ñ—Ä–∞–≤–∞–Ω–Ω—è –ø–∞–∫–∞–∑–∞–º</h1>

–ö—ñ—Ä–∞–≤–∞—Ü—å –ø–∞–∫–∞–∑–∞–º –º–æ–∂–Ω–∞ –∑ –¥–∞–ø–∞–º–æ–≥–∞–π –Ω–∞—Å—Ç—É–ø–Ω—ã—Ö –∫–ª–∞–≤—ñ—à:

–ö–ª–∞–≤—ñ—à–∞ Esc: –°–∫–æ–Ω—á—ã—Ü—å –ø–∞–∫–∞–∑.
–°—Ç—Ä—ç–ª–∫–∞ —û–≤–µ—Ä—Ö, 8 –Ω–∞ –ª—ñ—á–±–∞–≤–∞–π –∫–ª–∞–≤—ñ—è—Ç—É—Ä—ã, Backspace: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –ø–∞–ø—è—Ä—ç–¥–Ω—è–≥–∞ —Å–ª–∞–π–¥–∞.
–°—Ç—Ä—ç–ª–∫–∞ —û–Ω—ñ–∑, 2 –Ω–∞ –ª—ñ—á–±–∞–≤–∞–π –∫–ª–∞–≤—ñ—è—Ç—É—Ä—ã, –ü—Ä–∞–±–µ–ª: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞–≥–∞ —Å–ª–∞–π–¥–∞.
–°—Ç—Ä—ç–ª–∫–∞ —û–ª–µ–≤–∞, 4 –Ω–∞ –ª—ñ—á–±–∞–≤–∞–π –∫–ª–∞–≤—ñ—è—Ç—É—Ä—ã: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –ø–∞–ø—è—Ä—ç–¥–Ω—è–≥–∞ —ç–ª–µ–º–µ–Ω—Ç–∞ —û –Ω–∞–±–æ—Ä—ã.
–°—Ç—Ä—ç–ª–∫–∞ —û–ø—Ä–∞–≤–∞, 6 –Ω–∞ –ª—ñ—á–±–∞–≤–∞–π –∫–ª–∞–≤—ñ—è—Ç—É—Ä—ã: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞–≥–∞ —ç–ª–µ–º–µ–Ω—Ç–∞ —û –Ω–∞–±–æ—Ä—ã.
–ö–ª–∞–≤—ñ—à–∞ Q: –î–∞–¥–∞—Ü—å –Ω–æ–≤—ã —Å–ø–µ—û —É –±—è–≥—É—á—É—é –ø—Ä—ç–∑–µ–Ω—Ç–∞—Ü—ã—é.
–ö–ª–∞–≤—ñ—à–∞ A: –î–∞–¥–∞—Ü—å –Ω–æ–≤—É—é –∞–±–≤–µ—Å—Ç–∫—É.
–ö–ª–∞–≤—ñ—à–∞ S: –î–∞–¥–∞—Ü—å –Ω–æ–≤—É—é —Ü—ã—Ç–∞—Ç—É –∑ –ë—ñ–±–ª—ñ—ñ —û –±—è–≥—É—á—É—é –ø—Ä—ç–∑–µ–Ω—Ç–∞—Ü—ã—é.
W Key: –ü–µ—Ä–∞—Ö–æ–¥ –¥–∞ –±–µ–ª–∞–≥–∞ —ç–∫—Ä–∞–Ω–∞.
K Key: –ü–µ—Ä–∞—Ö–æ–¥ –¥–∞ —á–æ—Ä–Ω–∞–≥–∞ —ç–∫—Ä–∞–Ω–∞.
L Key: –ü–∞–∫–∞–∑–∞—Ü—å –ª–∞–≥–∞—Ç—ã–ø OpenSong.
H Key: –°—Ö–∞–≤–∞—Ü—å —Ç—ç–∫—Å—Ç, –ø–∞–∫–∞–∑–≤–∞—Ü—å —Ç–æ–ª—å–∫—ñ —Ñ–æ–Ω.
F Key: –ó–∞–º–∞—Ä–æ–∑—ñ—Ü—å –ø–∞–∫–∞–∑ (–ø–∞—û–∑–∞). (–≥.–∑–Ω. –ø—Ä–∞–¥—É—Ö—ñ–ª—ñ—Ü—å –ø–∞–∫–∞–∑ –Ω–æ–≤–∞–≥–∞ —Å–ª–∞–π–¥–∞, —à—Ç–æ –¥–∞–∑–≤–∞–ª—è–µ –∑–º—è–Ω—ñ—Ü—å –±—è–≥—É—á—ã —Å–ª–∞–π–¥ –±–µ–∑ —É–ø–ª—ã–≤—É –Ω–∞ –ø—Ä—ç–∑–µ–Ω—Ç–∞—Ü—ã—é. –ù–∞—Ü—ñ—Å–Ω—ñ—Ü–µ N, –∫–∞–± –ø—Ä–∞—Ü—è–≥–Ω—É—Ü—å –ø–∞–∫–∞–∑).
N Key: –ó–≤—ã—á–∞–π–Ω—ã —Ä—ç–∂—ã–º (–≤—è—Ä—Ç–∞–Ω–Ω–µ –∑ —á–æ—Ä–Ω–∞–≥–∞ —ç–∫—Ä–∞–Ω—É —ñ –≥.–¥.)
–ö–ª–∞–≤—ñ—à–∞ B: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –±—Ä—ã–¥–∂–∞ (–ø–∞–∑–Ω–∞—á–∞–Ω–∞–≥–∞ [B#] —É —Ä—ç–¥–∞–∫—Ç–∞—Ä—ã —Å–ø–µ–≤–∞—û). –ö–∞–ª—ñ –º–∞–µ—Ü—Ü–∞ –Ω–µ–∫–∞–ª—å–∫—ñ –±—Ä—ã–¥–∂–∞—û, –ø—Ä—ã –Ω–∞—Ü—ñ—Å–∫–∞–Ω–Ω—ñ –∫–ª–∞–≤—ñ—à—ã B –∞–¥–±—É–¥–∑–µ—Ü—Ü–∞ –ø–µ—Ä–∞—Ö–æ–¥ –¥–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞–≥–∞ –±—Ä—ã–¥–∂–∞.
–ö–ª–∞–≤—ñ—à–∞ C: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –ø—Ä—ã–ø–µ–≤—É (–ø–∞–∑–Ω–∞—á–∞–Ω–∞–≥–∞ [C#] —É —Ä—ç–¥–∞–∫—Ç–∞—Ä—ã —Å–ø–µ–≤–∞—û). –ö–∞–ª—ñ –º–∞–µ—Ü—Ü–∞ –Ω–µ–∫–∞–ª—å–∫—ñ –ø—Ä—ã–ø–µ–≤–∞—û, –ø—Ä—ã –Ω–∞—Ü—ñ—Å–∫–∞–Ω–Ω—ñ –∫–ª–∞–≤—ñ—à—ã C –∞–¥–±—É–¥–∑–µ—Ü—Ü–∞ –ø–µ—Ä–∞—Ö–æ–¥ –¥–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞–≥–∞ –ø—Ä—ã–ø–µ–≤—É.
–ö–ª–∞–≤—ñ—à–∞ T: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ —Ç—ç–≥–∞ (–ø–∞–∑–Ω–∞—á–∞–Ω–∞–≥–∞ [T#] —É —Ä—ç–¥–∞–∫—Ç–∞—Ä—ã —Å–ø–µ–≤–∞—û). –ö–∞–ª—ñ –º–∞–µ—Ü—Ü–∞ –Ω–µ–∫–∞–ª—å–∫—ñ —Ç—ç–≥–∞—û, –ø—Ä—ã –Ω–∞—Ü—ñ—Å–∫–∞–Ω–Ω—ñ –∫–ª–∞–≤—ñ—à—ã T –∞–¥–±—É–¥–∑–µ—Ü—Ü–∞ –ø–µ—Ä–∞—Ö–æ–¥ –¥–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞–≥–∞ —Ç—ç–≥–∞.
–ö–ª–∞–≤—ñ—à–∞ P: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –ø–µ—Ä–∞–¥–ø—Ä—ã–ø–µ–≤—É (–ø–∞–∑–Ω–∞—á–∞–Ω–∞–≥–∞ [P#] —É —Ä—ç–¥–∞–∫—Ç–∞—Ä—ã —Å–ø–µ–≤–∞—û). –ö–∞–ª—ñ –º–∞–µ—Ü—Ü–∞ –Ω–µ–∫–∞–ª—å–∫—ñ –ø–µ—Ä–∞–¥–ø—Ä—ã–ø–µ–≤–∞—û, –ø—Ä—ã –Ω–∞—Ü—ñ—Å–∫–∞–Ω–Ω—ñ –∫–ª–∞–≤—ñ—à—ã P –∞–¥–±—É–¥–∑–µ—Ü—Ü–∞ –ø–µ—Ä–∞—Ö–æ–¥ –¥–∞ –Ω–∞—Å—Ç—É–ø–Ω–∞–≥–∞ –ø–µ—Ä–∞–¥–ø—Ä—ã–ø–µ–≤—É.
–ö–ª–∞–≤—ñ—à—ã 1-9: –ü–µ—Ä–∞–π—Å—Ü—ñ –¥–∞ –∫—É–ø–ª–µ—Ç–∞ ‚Ññ (–ø–∞–∑–Ω–∞—á–∞–Ω–∞–≥–∞ [V#] —É —Ä—ç–¥–∞–∫—Ç–∞—Ä—ã —Å–ø–µ–≤–∞—û).

–ó–≤—è—Ä–Ω—ñ—Ü–µ —û–≤–∞–≥—É, —à—Ç–æ —Ñ—É–Ω–∫—Ü—ã—è –∫–ª–∞–≤—ñ—à –ª—ñ—á–±–∞–≤–∞–π –∫–ª–∞–≤—ñ—è—Ç—É—Ä—ã –∑–∞–ª–µ–∂—ã—Ü—å –∞–¥ —Å—Ç–∞–Ω—É –∞–∫—Ç—ã—û–Ω–∞—Å—Ü—ñ –∫–ª–∞–≤—ñ—à—ã NumLock.

]]></keys>

    <format><![CDATA[
<h1>–§–∞—Ä–º–∞—Ç –∞–∫–æ—Ä–¥–∞—û/—Ç—ç–∫—Å—Ç—É —û OpenSong</h1>

–°–ø–µ–≤—ã —Ä–∞–∑–±—ñ–≤–∞—é—Ü—Ü–∞ –Ω–∞ –Ω–∞—Å—Ç—É–ø–Ω—ã—è —Ç—ã–ø—ã —Ä–∞–¥–∫–æ—û: —Ä–∞–¥–∫—ñ —Ä–∞–∑–¥–∑–µ–ª–∞—û, —Ä–∞–¥–∫—ñ –∞–∫–æ—Ä–¥–∞—û, —Ä–∞–¥–∫—ñ —Ç—ç–∫—Å—Ç—É —ñ —Ä–∞–¥–∫—ñ –∫–∞–º–µ–Ω—Ç–∞—Ä—ã—è—û. –ü—Ä–æ—Å—Ç—ã —Å–ø–µ—û –º–æ–∂–∞ –≤—ã–≥–ª—è–¥–∞—Ü—å –Ω–∞—Å—Ç—É–ø–Ω—ã–º —á—ã–Ω–∞–º:
<pre><b>
;–ù–µ –∑–∞–±—É–¥–∑—å—Ü–µ –ø—Ä–∞—Å–ø—è–≤–∞—Ü—å –∫—É–ø–ª–µ—Ç –¥–≤–æ–π—á—ã.
